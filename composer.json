{
	"name": "salesagility/suitecrm",
	"description": "SuiteCRM",
	"author": [],
	"license": "GPL-3.0",
	"minimum-stability": "dev",
	"config": {
		"vendor-dir": "vendor",
		"platform": {
			"php": "5.5.9"
		}
	},
	"repositories": [
		{
			"type":"package",
			"package": {
				"name": "xtemplate/xtemplate",
				"version":"master",
				"source": {
					"url": "http://github.com/xtemplate/xtemplate.git",
					"type": "git",
					"reference":"master"
				}
			}
		}
	],
	"require": {
		"slim/slim": "3.9.2",
		"tuupola/slim-jwt-auth": "2.4.0",
		"psr/log": "1.0.x-dev",
		"league/oauth2-server": "5.1.6",
		"justinrainbow/json-schema": "5.0.x-dev",
		"onelogin/php-saml": "3.0.0.x-dev",
		"google/recaptcha": "1.1.3",

		"container-interop/container-interop": "^1.2",
		"defuse/php-encryption": "*",
		"firebase/php-jwt": "^3.0 || ^4.0 || ^5.0",
		"lcobucci/jwt": "3.1.x-dev",
		"league/event": "2.2.x-dev",
		"league/url": "3.x-dev",
		"nikic/fast-route": "*",
		"paragonie/random_compat": "2.0.12",
		"pimple/pimple": "3.2.x-dev",
		"psr/http-message": "1.0.x-dev",
		"psr/container": "1.0.x-dev",
		"robrichards/xmlseclibs": ">=3.0",
		"symfony/polyfill-mbstring": "1.7.0",
		"true/punycode": "^2.0",
		"zendframework/zendframework": ">=1.10.8",
		"xtemplate/xtemplate": "master",
		"facebook/php-sdk": ">=3.0.0",
		"smarty/smarty": ">=2.6.0",
		"ezyang/htmlpurifier": "4.6.0",
		"fergusean/nusoap": ">=0.6.5",
		"pclzip/pclzip": ">=2.4",
		"phpmailer/phpmailer": "5.2.25",
		"tecnickcom/tcpdf": ">=4.6.013"
	},
  "require-dev": {
    "leafo/scssphp": "dev-master",
    "mikey179/vfsStream": "1.4.*",
    "codeception/codeception": "^2.0",
    "fzaninotto/Faker": "^1.6",
    "jeroendesloovere/vcard": "v1.5",
    "browserstack/browserstack-local": "dev-master"
  },
  "scripts": {},
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "SuiteCRM\\": [
        "lib/",
        "include/",
        "tests/SuiteCRM/",
        "tests/unit/lib/SuiteCRM/"
      ],
      "SuiteCRM\\Custom\\": [
        "custom/lib"
      ]
    }
  },
  "extra": {}
}